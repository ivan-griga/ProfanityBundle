{
    "name": "hottomali/profanity-bundle",
    "description": "Checker for bad words.",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Ivan Griga",
            "email": "grigaivan2@gmail.com"
        },
        {
            "name": "Aron Beal",
            "email": "aron.beal.biz@gmail.com"
        }
    ],
    "keywords": ["symfony", "bad", "words", "bundle", "profanity", "filter", "censor"],
    "license": [
        "MIT"
